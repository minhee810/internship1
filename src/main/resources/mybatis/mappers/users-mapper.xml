<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

        <mapper namespace="usersMapper">
        
        <insert id="join">
        	INSERT INTO USERS
        	VALUES (
        		users_seq.nexval,
        		#{username},
        		#{email},
        		#{password},
        		#{phone},
        		#{address},
        		#{detailAddress},
        		#{zipCode},
        		#{note},
        		#{createdDate},
        		#{modifiedDate},
        		#{isDeleted}
        	)
        </insert>
        </mapper>